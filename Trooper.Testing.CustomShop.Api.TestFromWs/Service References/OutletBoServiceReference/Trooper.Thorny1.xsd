<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Trooper.Thorny" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Trooper.Thorny" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="MessageAlertLevel">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Error" />
      <xs:enumeration value="Warning" />
      <xs:enumeration value="Note" />
      <xs:enumeration value="Success" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="MessageAlertLevel" nillable="true" type="tns:MessageAlertLevel" />
  <xs:simpleType name="SaveChangeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Add" />
      <xs:enumeration value="Update" />
      <xs:enumeration value="None" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="SaveChangeType" nillable="true" type="tns:SaveChangeType" />
</xs:schema>